CREATE DATABASE EJERCICIOSQL

USE EJERCICIOSQL

CREATE TABLE COLEGIO(
ID_Colegio INT NOT NULL,
Direccion_C NVARCHAR(50) NOT NULL,
Telefono INT NOT NULL,
Maestros NVARCHAR (50) NOT NULL,
Grados INT NOT NULL,
Alumnos INT NOT NULL
PRIMARY KEY (ID_Colegio)
)

SELECT * FROM COLEGIO;

INSERT INTO COLEGIO(
ID_Colegio,
Direccion_C,
Telefono,
Maestros,
Grados,
Alumnos
)
